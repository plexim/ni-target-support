"static double SpdEst;\n"
"static double SpdFiltPrev;\n"
"\n"
"/*Params*/\n"
"static double SampleTime;\n"
"static double Gamma;\n"
"static double EncoderPairs;\n"
"static double RolloverThreshold;\n"
"\n"
"#define Count  InputSignal(0,0)\n"
"#define Enable   InputSignal(0,1)\n"
"#define SpdFilt  OutputSignal(0,0)\n"
                Show          off
              }
              Parameter {
                Variable      "StartFcn"
                Value         "CountPrev=Count;\n"
"DeltaCount=0;\n"
"SpdEst=0;\n"
"SpdFilt=0;\n"
"SpdFiltPrev=0;\n"
"\n"
"SampleTime = ParamRealData(0, 0);\n"
"Gamma = ParamRealData(1, 0);\n"
"EncoderPairs=ParamRealData(2, 0);\n"
"RolloverThreshold=EncoderPairs/2;\n"
"\n"
"if (SampleTime<=0)\n"
"{\n"
"\tSetErrorMessage(\"Sample time for speed estimation (Tdisc) must be greater "
"than zero\");\n"
"}\n"
"if (Gamma<=0)\n"
"{\n"
"\tSetErrorMessage(\"Filter parameter for speed estimation (Gamma) must be gre"
"ater than zero\");\n"
"}"
                Show          off
              }
              Parameter {
                Variable      "OutputFcn"
                Value         "if(!Enable)\n"
"{\n"
"\tDeltaCount=Count-CountPrev;\n"
"\tif(DeltaCount<-RolloverThreshold) /*Forward Rollover*/\n"
"\t{\n"
"\t\tDeltaCount=(DeltaCount+EncoderPairs);\n"
"\t}\n"
"\telse if (DeltaCount>RolloverThreshold) /*Reverse Rollover*/\n"
"\t{\n"
"\t\tDeltaCount=DeltaCount-(EncoderPairs);\n"
"\t}\n"
"\tSpdEst=DeltaCount;  //Estimate in counts\n"
"\tCountPrev=Count;\t\n"
"\n"
"\n"
"\t/*First order IIR*/\n"
"\tSpdFilt=(1-Gamma)*SpdEst+Gamma*SpdFiltPrev;\n"
"\tSpdFiltPrev=SpdFilt;\n"
"}\n"
                Show          off
